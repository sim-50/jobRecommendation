<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchJob</servlet-name>
  	<display-name>SearchJob</display-name>
  	<description></description>
  	<servlet-class>rpc.SearchJob</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RecommendJob</servlet-name>
  	<display-name>RecommendJob</display-name>
  	<description></description>
  	<servlet-class>rpc.RecommendJob</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ItemHistory</servlet-name>
  	<display-name>ItemHistory</display-name>
  	<description></description>
  	<servlet-class>rpc.ItemHistory</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>rpc.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>rpc.Logout</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>rpc.Register</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchJob</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RecommendJob</servlet-name>
  	<url-pattern>/recommendation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemHistory</servlet-name>
  	<url-pattern>/history</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
</web-app>
